<%= simple_form_for(@car) do |f| %>
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :color %>
  <%= f.input :address %>
  <%= f.input :price_per_day %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>

<p> <%= link_to "Return to all cars", cars_path, class:"btn btn-primary mt-3" %> </p>
