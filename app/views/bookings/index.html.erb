<% @bookings.each do |booking|%>
  <%=render "components/card", car:booking.car%>
  <div>
    <%=booking.start_date.strftime("%d %m %y")%>
  </div>
  <div>
    <%=booking.end_date.strftime("%d %m %y")%>
  </div>
  <div>
    <%=booking.price_total%>
  </div>
  <%= link_to "edit booking", edit_booking_path(booking), class:"btn btn-warning" %>

<% end %>
