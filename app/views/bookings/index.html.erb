<% @bookings.each do |booking|%>
  <%= render "components/bookingcard", car:booking.car, booking:booking%>
  <%= link_to "Edit Booking", edit_booking_path(booking), class:"btn btn-warning mt-3" %>
  <%= link_to "Delete This Booking", booking_path(booking),  data: {turbo_method: :delete, turbo_confirm: "Delete this booking?"}, class:"btn btn-danger mt-3"%>
<% end %>
