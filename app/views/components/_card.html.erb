<div id="<%= dom_id car %>">
  <%=link_to car_path(car), class:"card" do  %>
    <div>
      <%= car.color %>
    </div>
    <div>
      <%= car.model %>
    </div>
    <div>
      <%= car.brand%>
    </div>
    <div>
      <%= car.price_per_day%>
    </div>
      <div>
      <%= image_tag car.image %>
    </div>
    <div>
      <%= car.user.email%>
    </div>
  <% end %>
</div>
